# Microsoft Fabric Deployment Configuration
# Copy this file to .env and fill in your values

# Azure AD Tenant ID
FABRIC_TENANT_ID=your-tenant-id-here

# Service Principal Client ID
FABRIC_CLIENT_ID=your-client-id-here

# Service Principal Client Secret
FABRIC_CLIENT_SECRET=your-client-secret-here

# Fabric Workspace Name (optional, defaults to config.json)
FABRIC_WORKSPACE_NAME=KoladaWorkspace
